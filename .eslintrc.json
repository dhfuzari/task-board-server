{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "global-require": 1,
      "import/no-dynamic-require": 1,
      "no-underscore-dangle": 1,
      "no-console": 1,
      "consistent-return": 1
    }
}
